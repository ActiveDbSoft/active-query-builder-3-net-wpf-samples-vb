<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:ActiveQueryBuilder.View.WPF;assembly=ActiveQueryBuilder.View.WPF"
        xmlns:core="clr-namespace:ActiveQueryBuilder.Core;assembly=ActiveQueryBuilder.Core"
        mc:Ignorable="d"
        Title="Consolidated Metadata Container" Height="450" Width="800">
    <Grid>
        <wpf:QueryBuilder x:Name="QBuilder">
            <wpf:QueryBuilder.SyntaxProvider>
                <core:MSSQLSyntaxProvider/>
            </wpf:QueryBuilder.SyntaxProvider>
        </wpf:QueryBuilder>
    </Grid>
</Window>
